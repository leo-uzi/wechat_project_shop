/* pages/feedback/feedback.wxss */
page .fb_main {
  height: 100vh;
  margin-top: 0;
  font-size: 28rpx;
  background-color: #edf0ed;
  padding: 10rpx 10rpx;
  color: #535353;
}
page .fb_main .fb_tip {
  margin-top: 20rpx;
  display: flex;
  flex-wrap: wrap;
}
page .fb_main .fb_tip .fb_tip_item {
  background-color: #fff;
  width: 30%;
  text-align: center;
  padding: 10rpx 20rpx;
  border: solid 1rpx #cecece;
  border-radius: 8rpx;
  margin: 10rpx;
}
page .fb_main .fb_content {
  margin-top: 25rpx;
  border: solid 1rpx #cecece;
  background-color: #fff;
  padding: 15rpx;
}
page .fb_main .fb_content .fb_text_add {
  display: flex;
  flex-wrap: wrap;
}
page .fb_main .fb_content .fb_text_add .fb_add {
  margin: 0;
  width: 90rpx;
  height: 90rpx;
  border: #cecece solid 1rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #cecece;
  font-size: 50rpx;
}
page .fb_main .fb_content .fb_text_add .add_img_item {
  margin-left: 18rpx;
}
page .fb_main .fb_bottom {
  font-size: 30rpx;
  margin-top: 25rpx;
  display: flex;
  justify-content: flex-end;
}
page .fb_main .fb_bottom button {
  width: 25%;
  padding: 15rpx;
  color: #fff;
  background-color: var(--themeColor);
  margin: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
page .fb_main .fb_bottom button text {
  font-weight: 400;
}
