/**index.wxss**/
.swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.swiper swiper navigator image {
  width: 100%;
}
.index_cate {
  display: flex;
}
.index_cate .cate {
  flex: 1;
  padding: 20rpx;
}
.index_cate .cate navigator image {
  width: 100%;
}
.index_floor .floor .floor_title image {
  width: 100%;
}
.index_floor .floor .floor_content .content {
  overflow: hidden;
  float: left;
  width: 33.33%;
}
.index_floor .floor .floor_content .content:nth-last-child(-n+4) {
  height: 27.70215517vw;
  border-left: 10rpx solid #fff;
}
.index_floor .floor .floor_content .content:nth-child(2) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor .floor_content .content:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor .floor_content .content image {
  width: 100%;
  height: 100%;
}
